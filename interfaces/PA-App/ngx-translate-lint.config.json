{
  "tsconfig": "./",
  "project": "./src/app/**/*.{html,ts}",
  "languages": "./src/assets/i18n/en.json",
  "rules": {
    "keysOnViews": "warning",
    "zombieKeys": "error",
    "misprintKeys": "warning",
    "emptyKeys": "warning",
    "deepSearch": "enable",
    "maxWarning": "0",
    "ignoredKeys": [
      "_dir",
      "personal.select-language.introduction.(.*)",
      "personal.select-language.language.(.*)"
    ],
    "ignoredMisprintKeys": []
  }
}
